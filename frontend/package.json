    "@wagmi/core": "^1.4.6",
    "country-flag-icons": "^1.5.7",
    "i18next": "^23.7.3",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.1",
    "jsonata": "^2.0.3",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-cookie": "^6.1.1",
    "react-dom": "^18.2.0",
